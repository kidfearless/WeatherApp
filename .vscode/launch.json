	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Android On Emulator",
			"type": "cordova",
			"request": "launch",
			"platform": "android",
			"target": "emulator",
			"sourceMaps": true,
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Run Browser",
			"type": "cordova",
			"request": "launch",
			"platform": "browser",
			"target": "firefox",
			"sourceMaps": true,
			"cwd": "${workspaceFolder}"
		},
	]
}